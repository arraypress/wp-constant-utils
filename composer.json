{
  "name": "arraypress/wp-constant-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for managing and defining constants safely",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-constant-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\ConstantUtils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-constant-utils/issues",
    "source": "https://github.com/arraypress/wp-constant-utils"
  },
  "keywords": [
    "wordpress",
    "constants",
    "utilities",
    "plugin",
    "theme",
    "arraypress"
  ]
}